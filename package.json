      "!backend/tests/**",
      "!backend/scripts/**",
      "!backend/node_modules/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testTimeout": 30000,
    "verbose": true,
    "forceExit": true,
    "clearMocks": true,
    "resetMocks": true,
    "restoreMocks": true
