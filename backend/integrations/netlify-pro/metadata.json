{
  "name": "netlify_pro",
  "displayName": "Netlify Pro",
  "version": "1.0.0",
  "description": "Premium Netlify integration with 28 advanced endpoints for JAMstack deployments, forms, functions, and analytics",
  "category": "deployment",
  "tier": "premium",
  "icon": "https://www.netlify.com/favicon.ico",
  "authentication": {"type": "bearer", "tokenType": "Bearer"},
  "baseUrl": "https://api.netlify.com/api/v1",
  "rateLimit": {"perMinute": 500},
  "premiumFeatures": ["Advanced analytics", "Form handling", "Serverless functions", "Split testing", "Deploy previews", "Build hooks", "Identity management", "Large media"],
  "endpoints": [
    {"id": "list_sites", "name": "List Sites", "method": "GET", "path": "/sites"},
    {"id": "get_site", "name": "Get Site", "method": "GET", "path": "/sites/{site_id}"},
    {"id": "create_site", "name": "Create Site", "method": "POST", "path": "/sites"},
    {"id": "update_site", "name": "Update Site", "method": "PATCH", "path": "/sites/{site_id}"},
    {"id": "delete_site", "name": "Delete Site", "method": "DELETE", "path": "/sites/{site_id}"},
    {"id": "list_deploys", "name": "List Deploys", "method": "GET", "path": "/sites/{site_id}/deploys"},
    {"id": "get_deploy", "name": "Get Deploy", "method": "GET", "path": "/deploys/{deploy_id}"},
    {"id": "create_deploy", "name": "Create Deploy", "method": "POST", "path": "/sites/{site_id}/deploys"},
    {"id": "cancel_deploy", "name": "Cancel Deploy", "method": "POST", "path": "/deploys/{deploy_id}/cancel"},
    {"id": "restore_deploy", "name": "Restore Deploy", "method": "POST", "path": "/sites/{site_id}/deploys/{deploy_id}/restore"},
    {"id": "list_forms", "name": "List Forms", "method": "GET", "path": "/sites/{site_id}/forms"},
    {"id": "get_form", "name": "Get Form", "method": "GET", "path": "/forms/{form_id}"},
    {"id": "list_submissions", "name": "List Form Submissions", "method": "GET", "path": "/forms/{form_id}/submissions"},
    {"id": "list_functions", "name": "List Functions", "method": "GET", "path": "/sites/{site_id}/functions"},
    {"id": "get_function", "name": "Get Function", "method": "GET", "path": "/sites/{site_id}/functions/{function_name}"},
    {"id": "list_build_hooks", "name": "List Build Hooks", "method": "GET", "path": "/sites/{site_id}/build_hooks"},
    {"id": "create_build_hook", "name": "Create Build Hook", "method": "POST", "path": "/sites/{site_id}/build_hooks"},
    {"id": "delete_build_hook", "name": "Delete Build Hook", "method": "DELETE", "path": "/sites/{site_id}/build_hooks/{id}"},
    {"id": "get_analytics", "name": "Get Analytics", "method": "GET", "path": "/sites/{site_id}/analytics"},
    {"id": "list_env_vars", "name": "List Environment Variables", "method": "GET", "path": "/accounts/{account_id}/env"},
    {"id": "create_env_var", "name": "Create Environment Variable", "method": "POST", "path": "/accounts/{account_id}/env"},
    {"id": "update_env_var", "name": "Update Environment Variable", "method": "PATCH", "path": "/accounts/{account_id}/env/{key}"},
    {"id": "delete_env_var", "name": "Delete Environment Variable", "method": "DELETE", "path": "/accounts/{account_id}/env/{key}"},
    {"id": "list_split_tests", "name": "List Split Tests", "method": "GET", "path": "/sites/{site_id}/traffic_splits"},
    {"id": "create_split_test", "name": "Create Split Test", "method": "POST", "path": "/sites/{site_id}/traffic_splits"},
    {"id": "list_snippets", "name": "List Snippets", "method": "GET", "path": "/sites/{site_id}/snippets"},
    {"id": "create_snippet", "name": "Create Snippet", "method": "POST", "path": "/sites/{site_id}/snippets"},
    {"id": "get_account", "name": "Get Account", "method": "GET", "path": "/accounts/{account_id}"}
  ]
}
