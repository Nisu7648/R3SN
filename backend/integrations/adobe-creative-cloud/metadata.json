      "name": "Get Library Elements",
      "method": "GET",
      "path": "/libraries/{library_id}/elements",
      "description": "Get elements in library"
    },
    {
      "id": "cc_libraries_add_element",
      "name": "Add Library Element",
      "method": "POST",
      "path": "/libraries/{library_id}/elements",
      "description": "Add element to library"
    },
    {
      "id": "cc_libraries_update_element",
      "name": "Update Library Element",
      "method": "PUT",
      "path": "/libraries/{library_id}/elements/{element_id}",
      "description": "Update library element"
    },
    {
      "id": "cc_libraries_delete_element",
      "name": "Delete Library Element",
      "method": "DELETE",
      "path": "/libraries/{library_id}/elements/{element_id}",
      "description": "Delete library element"
    },
    {
      "id": "cc_storage_list_files",
      "name": "List Cloud Files",
      "method": "GET",
      "path": "/files",
      "description": "List Creative Cloud files"
    },
    {
      "id": "cc_storage_upload",
      "name": "Upload File",
      "method": "POST",
      "path": "/files",
      "description": "Upload file to cloud"
    },
    {
      "id": "cc_storage_download",
      "name": "Download File",
      "method": "GET",
