      "method": "POST",
      "path": "/pdfServices/insertPages",
      "description": "Insert pages into PDF"
    },
    {
      "id": "pdf_delete_pages",
      "name": "Delete PDF Pages",
      "method": "POST",
      "path": "/pdfServices/deletePages",
      "description": "Delete pages from PDF"
    },
    {
      "id": "pdf_rotate_pages",
      "name": "Rotate PDF Pages",
      "method": "POST",
      "path": "/pdfServices/rotatePages",
      "description": "Rotate PDF pages"
