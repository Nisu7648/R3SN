{
  "name": "adobe-creative-cloud",
  "displayName": "Adobe Creative Cloud",
  "version": "1.0.0",
  "description": "Complete Adobe Creative Cloud integration with 60+ premium endpoints for Photoshop, Illustrator, Lightroom, InDesign, XD, and asset management with full access to all premium features",
  "category": "design",
  "icon": "https://cdn.worldvectorlogo.com/logos/adobe-creative-cloud.svg",
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "scopes": [
      "openid",
      "creative_sdk",
      "AdobeID",
      "read_organizations",
      "additional_info.projectedProductContext"
    ]
  },
  "baseUrl": "https://image.adobe.io",
  "rateLimit": {
    "quota": 5000,
    "perDay": 5000,
    "concurrent": 50
  },
  "endpoints": [
    {
      "id": "photoshop_remove_background",
      "name": "Remove Background (Photoshop)",
      "method": "POST",
      "path": "/sensei/cutout",
      "description": "AI-powered background removal"
    },
    {
      "id": "photoshop_auto_tone",
      "name": "Auto Tone (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/autoTone",
      "description": "Automatically adjust image tone"
    },
    {
      "id": "photoshop_smart_crop",
      "name": "Smart Crop (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/smartCrop",
      "description": "AI-powered smart cropping"
    },
    {
      "id": "photoshop_apply_preset",
      "name": "Apply Preset (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/applyPreset",
      "description": "Apply Lightroom presets"
    },
    {
      "id": "photoshop_edit_text",
      "name": "Edit Text Layer (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/text",
      "description": "Edit text in PSD files"
    },
    {
      "id": "photoshop_replace_smart_object",
      "name": "Replace Smart Object (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/smartObject",
      "description": "Replace smart objects in PSD"
    },
    {
      "id": "photoshop_create_document",
      "name": "Create Document (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/documentCreate",
      "description": "Create new PSD document"
    },
    {
      "id": "photoshop_create_rendition",
      "name": "Create Rendition (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/renditionCreate",
      "description": "Generate image renditions"
    },
    {
      "id": "photoshop_apply_actions",
      "name": "Apply Actions (Photoshop)",
      "method": "POST",
      "path": "/pie/psdService/actionJSON",
      "description": "Apply Photoshop actions"
    },
    {
      "id": "photoshop_depth_blur",
      "name": "Depth Blur (Photoshop)",
      "method": "POST",
      "path": "/sensei/depthBlur",
      "description": "Apply depth-based blur effect"
    },
    {
      "id": "lightroom_auto_adjust",
      "name": "Auto Adjust (Lightroom)",
      "method": "POST",
      "path": "/lrService/autoTone",
      "description": "Auto adjust photo settings"
    },
    {
      "id": "lightroom_apply_preset",
      "name": "Apply Preset (Lightroom)",
      "method": "POST",
      "path": "/lrService/applyPreset",
      "description": "Apply Lightroom presets"
    },
    {
      "id": "lightroom_edit_photo",
      "name": "Edit Photo (Lightroom)",
      "method": "POST",
      "path": "/lrService/edit",
      "description": "Edit photo with adjustments"
    },
    {
      "id": "lightroom_straighten",
      "name": "Straighten (Lightroom)",
      "method": "POST",
      "path": "/lrService/straighten",
      "description": "Auto straighten images"
    },
    {
