      "path": "/pdfServices/extractPages",
      "description": "Extract specific pages"
    },
    {
      "id": "pdf_linearize",
      "name": "Linearize PDF",
      "method": "POST",
      "path": "/pdfServices/linearizePDF",
      "description": "Optimize for web viewing"
    },
    {
