{
  "name": "Google Sheets Complete",
  "id": "google-sheets-complete",
  "category": "productivity",
  "description": "Complete Google Sheets integration for spreadsheet management",
  "version": "1.0.0",
  "authentication": {
    "type": "oauth2",
    "fields": [
      {"name": "apiKey", "label": "API Key", "type": "password", "required": true},
      {"name": "accessToken", "label": "Access Token", "type": "password", "required": false}
    ]
  },
  "endpoints": [
    {"name": "getSpreadsheet", "method": "GET", "path": "/spreadsheets/:id"},
    {"name": "getValues", "method": "GET", "path": "/values"},
    {"name": "updateValues", "method": "PUT", "path": "/values"},
    {"name": "appendValues", "method": "POST", "path": "/values/append"},
    {"name": "createSpreadsheet", "method": "POST", "path": "/spreadsheets"}
  ],
  "pricing": {"model": "free", "details": "Free with Google account"},
  "documentation": "https://developers.google.com/sheets/api",
  "website": "https://sheets.google.com"
}
