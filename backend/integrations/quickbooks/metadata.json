      "method": "GET",
      "path": "/query?query=select * from Bill",
      "description": "Get all bills"
    },
    {
      "id": "create_bill",
      "name": "Create Bill",
      "method": "POST",
      "path": "/bill",
      "description": "Create a new bill"
    },
    {
      "id": "list_vendors",
      "name": "List Vendors",
