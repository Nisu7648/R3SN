{
  "name": "quickbooks",
  "displayName": "QuickBooks",
  "version": "1.0.0",
  "description": "Complete QuickBooks integration with 45 endpoints for invoices, customers, expenses, payments, and financial reports",
  "category": "accounting",
  "icon": "https://cdn.worldvectorlogo.com/logos/quickbooks-1.svg",
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "scopes": [
      "com.intuit.quickbooks.accounting"
    ]
  },
  "baseUrl": "https://quickbooks.api.intuit.com/v3/company/{realmId}",
  "rateLimit": {
    "quota": 500,
    "perMinute": 500,
    "concurrent": 10
  },
  "endpoints": [
    {
      "id": "list_invoices",
      "name": "List Invoices",
      "method": "GET",
      "path": "/query?query=select * from Invoice",
      "description": "Get all invoices"
    },
    {
      "id": "get_invoice",
      "name": "Get Invoice",
      "method": "GET",
      "path": "/invoice/{invoice_id}",
      "description": "Get a specific invoice"
    },
    {
      "id": "create_invoice",
      "name": "Create Invoice",
      "method": "POST",
      "path": "/invoice",
      "description": "Create a new invoice"
    },
    {
      "id": "update_invoice",
      "name": "Update Invoice",
      "method": "POST",
      "path": "/invoice",
      "description": "Update an invoice"
    },
    {
      "id": "delete_invoice",
      "name": "Delete Invoice",
      "method": "POST",
      "path": "/invoice?operation=delete",
      "description": "Delete an invoice"
    },
    {
      "id": "send_invoice",
      "name": "Send Invoice",
      "method": "POST",
      "path": "/invoice/{invoice_id}/send",
      "description": "Send invoice via email"
    },
    {
      "id": "list_customers",
      "name": "List Customers",
      "method": "GET",
      "path": "/query?query=select * from Customer",
      "description": "Get all customers"
    },
    {
      "id": "get_customer",
      "name": "Get Customer",
      "method": "GET",
      "path": "/customer/{customer_id}",
      "description": "Get a specific customer"
    },
    {
      "id": "create_customer",
      "name": "Create Customer",
      "method": "POST",
      "path": "/customer",
      "description": "Create a new customer"
    },
    {
      "id": "update_customer",
      "name": "Update Customer",
      "method": "POST",
      "path": "/customer",
      "description": "Update a customer"
    },
    {
      "id": "delete_customer",
      "name": "Delete Customer",
      "method": "POST",
      "path": "/customer?operation=delete",
      "description": "Delete a customer"
    },
    {
      "id": "list_expenses",
      "name": "List Expenses",
      "method": "GET",
      "path": "/query?query=select * from Purchase",
      "description": "Get all expenses"
    },
    {
      "id": "get_expense",
      "name": "Get Expense",
      "method": "GET",
      "path": "/purchase/{expense_id}",
      "description": "Get a specific expense"
    },
    {
      "id": "create_expense",
      "name": "Create Expense",
      "method": "POST",
      "path": "/purchase",
      "description": "Create a new expense"
    },
    {
      "id": "update_expense",
      "name": "Update Expense",
      "method": "POST",
      "path": "/purchase",
      "description": "Update an expense"
    },
    {
      "id": "delete_expense",
      "name": "Delete Expense",
      "method": "POST",
      "path": "/purchase?operation=delete",
      "description": "Delete an expense"
    },
    {
      "id": "list_payments",
      "name": "List Payments",
      "method": "GET",
      "path": "/query?query=select * from Payment",
      "description": "Get all payments"
    },
    {
      "id": "get_payment",
