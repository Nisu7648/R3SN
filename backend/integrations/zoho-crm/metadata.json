{
  "name": "zoho_crm",
  "displayName": "Zoho CRM",
  "version": "1.0.0",
  "description": "Complete Zoho CRM integration with 25 endpoints for leads, contacts, accounts, deals, and tasks",
  "category": "crm",
  "icon": "https://www.zoho.com/favicon.ico",
  "authentication": {"type": "oauth2", "tokenType": "Bearer"},
  "baseUrl": "https://www.zohoapis.com/crm/v2",
  "rateLimit": {"perMinute": 100},
  "endpoints": [
    {"id": "list_leads", "name": "List Leads", "method": "GET", "path": "/Leads"},
    {"id": "get_lead", "name": "Get Lead", "method": "GET", "path": "/Leads/{id}"},
    {"id": "create_lead", "name": "Create Lead", "method": "POST", "path": "/Leads"},
    {"id": "update_lead", "name": "Update Lead", "method": "PUT", "path": "/Leads/{id}"},
    {"id": "delete_lead", "name": "Delete Lead", "method": "DELETE", "path": "/Leads/{id}"},
    {"id": "list_contacts", "name": "List Contacts", "method": "GET", "path": "/Contacts"},
    {"id": "get_contact", "name": "Get Contact", "method": "GET", "path": "/Contacts/{id}"},
    {"id": "create_contact", "name": "Create Contact", "method": "POST", "path": "/Contacts"},
    {"id": "update_contact", "name": "Update Contact", "method": "PUT", "path": "/Contacts/{id}"},
    {"id": "list_accounts", "name": "List Accounts", "method": "GET", "path": "/Accounts"},
    {"id": "get_account", "name": "Get Account", "method": "GET", "path": "/Accounts/{id}"},
    {"id": "create_account", "name": "Create Account", "method": "POST", "path": "/Accounts"},
    {"id": "update_account", "name": "Update Account", "method": "PUT", "path": "/Accounts/{id}"},
    {"id": "list_deals", "name": "List Deals", "method": "GET", "path": "/Deals"},
    {"id": "get_deal", "name": "Get Deal", "method": "GET", "path": "/Deals/{id}"},
    {"id": "create_deal", "name": "Create Deal", "method": "POST", "path": "/Deals"},
    {"id": "update_deal", "name": "Update Deal", "method": "PUT", "path": "/Deals/{id}"},
    {"id": "list_tasks", "name": "List Tasks", "method": "GET", "path": "/Tasks"},
    {"id": "create_task", "name": "Create Task", "method": "POST", "path": "/Tasks"},
    {"id": "list_notes", "name": "List Notes", "method": "GET", "path": "/Notes"},
    {"id": "create_note", "name": "Create Note", "method": "POST", "path": "/Notes"},
    {"id": "list_calls", "name": "List Calls", "method": "GET", "path": "/Calls"},
    {"id": "create_call", "name": "Create Call", "method": "POST", "path": "/Calls"},
    {"id": "list_users", "name": "List Users", "method": "GET", "path": "/users"},
    {"id": "search_records", "name": "Search Records", "method": "GET", "path": "/{module}/search"}
  ]
}
