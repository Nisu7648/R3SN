{
  "name": "google_colab_pro",
  "displayName": "Google Colab Pro",
  "version": "1.0.0",
  "description": "Premium Google Colab with 30 endpoints - FREE GPU/TPU Jupyter notebooks with unlimited runtime",
  "category": "cloud_compute",
  "tier": "premium",
  "icon": "https://colab.research.google.com/favicon.ico",
  "authentication": {"type": "oauth2", "provider": "google"},
  "baseUrl": "https://colab.research.google.com/api/v1",
  "rateLimit": {"perMinute": 300},
  "premiumFeatures": [
    "FREE GPU access (T4, P100, V100)",
    "FREE TPU access",
    "Background execution",
    "Priority compute",
    "Longer runtimes",
    "More RAM (up to 52GB)",
    "Google Drive integration",
    "GitHub integration",
    "Terminal access",
    "Custom environments"
  ],
  "computeTypes": ["CPU", "GPU (T4)", "GPU (P100)", "GPU (V100)", "TPU v2", "TPU v3"],
  "endpoints": [
    {"id": "list_notebooks", "name": "List Notebooks", "method": "GET", "path": "/notebooks"},
    {"id": "get_notebook", "name": "Get Notebook", "method": "GET", "path": "/notebooks/{notebook_id}"},
    {"id": "create_notebook", "name": "Create Notebook", "method": "POST", "path": "/notebooks"},
    {"id": "update_notebook", "name": "Update Notebook", "method": "PATCH", "path": "/notebooks/{notebook_id}"},
    {"id": "delete_notebook", "name": "Delete Notebook", "method": "DELETE", "path": "/notebooks/{notebook_id}"},
    {"id": "execute_cell", "name": "Execute Cell", "method": "POST", "path": "/notebooks/{notebook_id}/execute"},
    {"id": "get_runtime", "name": "Get Runtime", "method": "GET", "path": "/runtimes/{runtime_id}"},
    {"id": "create_runtime", "name": "Create Runtime", "method": "POST", "path": "/runtimes"},
    {"id": "connect_runtime", "name": "Connect Runtime", "method": "POST", "path": "/runtimes/{runtime_id}/connect"},
    {"id": "disconnect_runtime", "name": "Disconnect Runtime", "method": "POST", "path": "/runtimes/{runtime_id}/disconnect"},
    {"id": "restart_runtime", "name": "Restart Runtime", "method": "POST", "path": "/runtimes/{runtime_id}/restart"},
    {"id": "change_runtime_type", "name": "Change Runtime Type", "method": "POST", "path": "/runtimes/{runtime_id}/change-type"},
    {"id": "get_gpu_info", "name": "Get GPU Info", "method": "GET", "path": "/runtimes/{runtime_id}/gpu"},
    {"id": "get_tpu_info", "name": "Get TPU Info", "method": "GET", "path": "/runtimes/{runtime_id}/tpu"},
    {"id": "upload_file", "name": "Upload File", "method": "POST", "path": "/files/upload"},
    {"id": "download_file", "name": "Download File", "method": "GET", "path": "/files/{file_id}/download"},
    {"id": "list_files", "name": "List Files", "method": "GET", "path": "/files"},
    {"id": "delete_file", "name": "Delete File", "method": "DELETE", "path": "/files/{file_id}"},
    {"id": "mount_drive", "name": "Mount Google Drive", "method": "POST", "path": "/drive/mount"},
    {"id": "unmount_drive", "name": "Unmount Google Drive", "method": "POST", "path": "/drive/unmount"},
    {"id": "install_package", "name": "Install Package", "method": "POST", "path": "/packages/install"},
    {"id": "list_packages", "name": "List Packages", "method": "GET", "path": "/packages"},
    {"id": "get_system_info", "name": "Get System Info", "method": "GET", "path": "/system/info"},
    {"id": "get_resource_usage", "name": "Get Resource Usage", "method": "GET", "path": "/system/resources"},
    {"id": "share_notebook", "name": "Share Notebook", "method": "POST", "path": "/notebooks/{notebook_id}/share"},
    {"id": "clone_notebook", "name": "Clone Notebook", "method": "POST", "path": "/notebooks/{notebook_id}/clone"},
    {"id": "export_notebook", "name": "Export Notebook", "method": "GET", "path": "/notebooks/{notebook_id}/export"},
    {"id": "import_notebook", "name": "Import Notebook", "method": "POST", "path": "/notebooks/import"},
    {"id": "get_usage_stats", "name": "Get Usage Stats", "method": "GET", "path": "/usage"},
    {"id": "get_compute_units", "name": "Get Compute Units", "method": "GET", "path": "/compute-units"}
  ]
}
