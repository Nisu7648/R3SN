{
  "name": "Asana",
  "id": "asana",
  "category": "productivity",
  "description": "Project management and task tracking platform",
  "version": "1.0.0",
  "auth": {
    "type": "oauth2",
    "authUrl": "https://app.asana.com/-/oauth_authorize",
    "tokenUrl": "https://app.asana.com/-/oauth_token",
    "scope": "default",
    "clientId": "ASANA_CLIENT_ID",
    "clientSecret": "ASANA_CLIENT_SECRET"
  },
  "baseUrl": "https://app.asana.com/api/1.0",
  "features": [
    "Workspace management",
    "Project creation and management",
    "Task management",
    "Subtasks",
    "Sections",
    "Tags",
    "Comments",
    "Attachments",
    "Custom fields",
    "Portfolios",
    "Teams",
    "User management"
  ],
  "endpoints": {
    "workspaces": "/workspaces",
    "projects": "/projects",
    "tasks": "/tasks",
    "sections": "/projects/{project_id}/sections",
    "tags": "/tags",
    "users": "/users",
    "portfolios": "/portfolios",
    "teams": "/organizations/{organization_id}/teams",
    "customFields": "/custom_fields"
  },
  "rateLimit": {
    "requests": 1500,
    "period": "minute"
  },
  "pricing": {
    "free": true,
    "plans": ["Basic", "Premium", "Business", "Enterprise"]
  },
  "documentation": "https://developers.asana.com/docs",
  "status": "active",
  "lastUpdated": "2025-01-09"
}
