{
  "name": "algolia_premium",
  "displayName": "Algolia Premium",
  "version": "1.0.0",
  "description": "Premium Algolia integration with 25 advanced search endpoints including AI search, personalization, and analytics",
  "category": "search",
  "tier": "premium",
  "icon": "https://www.algolia.com/favicon.ico",
  "authentication": {"type": "api_key", "headers": ["X-Algolia-Application-Id", "X-Algolia-API-Key"]},
  "baseUrl": "https://{app_id}-dsn.algolia.net/1",
  "rateLimit": {"perSecond": 1000},
  "premiumFeatures": ["AI-powered search", "Personalization", "Advanced analytics", "A/B testing", "Query suggestions", "Federated search", "Dynamic re-ranking", "Visual editor"],
  "endpoints": [
    {"id": "search", "name": "Search", "method": "POST", "path": "/indexes/{index}/query"},
    {"id": "search_multiple", "name": "Search Multiple Indexes", "method": "POST", "path": "/indexes/*/queries"},
    {"id": "browse", "name": "Browse Index", "method": "POST", "path": "/indexes/{index}/browse"},
    {"id": "add_object", "name": "Add Object", "method": "POST", "path": "/indexes/{index}"},
    {"id": "update_object", "name": "Update Object", "method": "PUT", "path": "/indexes/{index}/{objectID}"},
    {"id": "partial_update", "name": "Partial Update", "method": "POST", "path": "/indexes/{index}/{objectID}/partial"},
    {"id": "delete_object", "name": "Delete Object", "method": "DELETE", "path": "/indexes/{index}/{objectID}"},
    {"id": "batch", "name": "Batch Operations", "method": "POST", "path": "/indexes/{index}/batch"},
    {"id": "clear_index", "name": "Clear Index", "method": "POST", "path": "/indexes/{index}/clear"},
    {"id": "delete_index", "name": "Delete Index", "method": "DELETE", "path": "/indexes/{index}"},
    {"id": "list_indexes", "name": "List Indexes", "method": "GET", "path": "/indexes"},
    {"id": "get_settings", "name": "Get Settings", "method": "GET", "path": "/indexes/{index}/settings"},
    {"id": "set_settings", "name": "Set Settings", "method": "PUT", "path": "/indexes/{index}/settings"},
    {"id": "get_synonyms", "name": "Get Synonyms", "method": "GET", "path": "/indexes/{index}/synonyms"},
    {"id": "save_synonym", "name": "Save Synonym", "method": "PUT", "path": "/indexes/{index}/synonyms/{objectID}"},
    {"id": "get_rules", "name": "Get Rules", "method": "GET", "path": "/indexes/{index}/rules"},
    {"id": "save_rule", "name": "Save Rule", "method": "PUT", "path": "/indexes/{index}/rules/{objectID}"},
    {"id": "get_personalization", "name": "Get Personalization Strategy", "method": "GET", "path": "/recommendation/personalization/strategy"},
    {"id": "set_personalization", "name": "Set Personalization Strategy", "method": "POST", "path": "/recommendation/personalization/strategy"},
    {"id": "get_ab_tests", "name": "Get A/B Tests", "method": "GET", "path": "/2/abtests"},
    {"id": "create_ab_test", "name": "Create A/B Test", "method": "POST", "path": "/2/abtests"},
    {"id": "get_analytics", "name": "Get Analytics", "method": "GET", "path": "/2/searches"},
    {"id": "get_top_searches", "name": "Get Top Searches", "method": "GET", "path": "/2/searches/noResults"},
    {"id": "get_query_suggestions", "name": "Get Query Suggestions", "method": "POST", "path": "/1/indexes/{index}/query"},
    {"id": "get_recommendations", "name": "Get Recommendations", "method": "POST", "path": "/1/indexes/*/recommendations"}
  ]
}
