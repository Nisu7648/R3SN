{
  "name": "together_ai",
  "displayName": "Together AI",
  "version": "1.0.0",
  "description": "Premium Together AI platform with 28 endpoints - FREE distributed GPU inference for LLMs at scale",
  "category": "gpu_cloud",
  "tier": "premium",
  "icon": "https://together.ai/favicon.ico",
  "authentication": {"type": "bearer", "tokenType": "Bearer"},
  "baseUrl": "https://api.together.xyz/v1",
  "rateLimit": {"perMinute": 600},
  "premiumFeatures": ["FREE LLM inference", "Distributed GPU clusters", "Fine-tuning", "Custom models", "Low latency", "High throughput", "Model hosting", "API access"],
  "endpoints": [
    {"id": "list_models", "name": "List Models", "method": "GET", "path": "/models"},
    {"id": "get_model", "name": "Get Model", "method": "GET", "path": "/models/{model}"},
    {"id": "create_completion", "name": "Create Completion", "method": "POST", "path": "/completions"},
    {"id": "create_chat_completion", "name": "Create Chat Completion", "method": "POST", "path": "/chat/completions"},
    {"id": "create_embedding", "name": "Create Embedding", "method": "POST", "path": "/embeddings"},
    {"id": "create_image", "name": "Create Image", "method": "POST", "path": "/images/generations"},
    {"id": "list_files", "name": "List Files", "method": "GET", "path": "/files"},
    {"id": "upload_file", "name": "Upload File", "method": "POST", "path": "/files"},
    {"id": "get_file", "name": "Get File", "method": "GET", "path": "/files/{file_id}"},
    {"id": "delete_file", "name": "Delete File", "method": "DELETE", "path": "/files/{file_id}"},
    {"id": "create_fine_tune", "name": "Create Fine-tune", "method": "POST", "path": "/fine-tunes"},
    {"id": "list_fine_tunes", "name": "List Fine-tunes", "method": "GET", "path": "/fine-tunes"},
    {"id": "get_fine_tune", "name": "Get Fine-tune", "method": "GET", "path": "/fine-tunes/{fine_tune_id}"},
    {"id": "cancel_fine_tune", "name": "Cancel Fine-tune", "method": "POST", "path": "/fine-tunes/{fine_tune_id}/cancel"},
    {"id": "list_fine_tune_events", "name": "List Fine-tune Events", "method": "GET", "path": "/fine-tunes/{fine_tune_id}/events"},
    {"id": "download_fine_tune", "name": "Download Fine-tune", "method": "GET", "path": "/fine-tunes/{fine_tune_id}/download"},
    {"id": "list_deployments", "name": "List Deployments", "method": "GET", "path": "/deployments"},
    {"id": "create_deployment", "name": "Create Deployment", "method": "POST", "path": "/deployments"},
    {"id": "get_deployment", "name": "Get Deployment", "method": "GET", "path": "/deployments/{deployment_id}"},
    {"id": "update_deployment", "name": "Update Deployment", "method": "PATCH", "path": "/deployments/{deployment_id}"},
    {"id": "delete_deployment", "name": "Delete Deployment", "method": "DELETE", "path": "/deployments/{deployment_id}"},
    {"id": "get_usage", "name": "Get Usage", "method": "GET", "path": "/usage"},
    {"id": "get_billing", "name": "Get Billing", "method": "GET", "path": "/billing"},
    {"id": "get_credits", "name": "Get Credits", "method": "GET", "path": "/credits"},
    {"id": "list_api_keys", "name": "List API Keys", "method": "GET", "path": "/api-keys"},
    {"id": "create_api_key", "name": "Create API Key", "method": "POST", "path": "/api-keys"},
    {"id": "delete_api_key", "name": "Delete API Key", "method": "DELETE", "path": "/api-keys/{key_id}"},
    {"id": "get_health", "name": "Get Health", "method": "GET", "path": "/health"}
  ]
}
