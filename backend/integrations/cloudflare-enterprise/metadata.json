{
  "name": "cloudflare_enterprise",
  "displayName": "Cloudflare Enterprise",
  "version": "1.0.0",
  "description": "Premium Cloudflare integration with 35 enterprise endpoints for CDN, security, workers, and analytics",
  "category": "infrastructure",
  "tier": "premium",
  "icon": "https://www.cloudflare.com/favicon.ico",
  "authentication": {"type": "api_key", "headers": ["X-Auth-Email", "X-Auth-Key"]},
  "baseUrl": "https://api.cloudflare.com/client/v4",
  "rateLimit": {"perMinute": 1200},
  "premiumFeatures": ["Advanced DDoS protection", "Workers KV", "Durable Objects", "Stream", "Images", "Load balancing", "Rate limiting", "Bot management"],
  "endpoints": [
    {"id": "list_zones", "name": "List Zones", "method": "GET", "path": "/zones"},
    {"id": "get_zone", "name": "Get Zone", "method": "GET", "path": "/zones/{id}"},
    {"id": "create_zone", "name": "Create Zone", "method": "POST", "path": "/zones"},
    {"id": "delete_zone", "name": "Delete Zone", "method": "DELETE", "path": "/zones/{id}"},
    {"id": "purge_cache", "name": "Purge Cache", "method": "POST", "path": "/zones/{id}/purge_cache"},
    {"id": "list_dns_records", "name": "List DNS Records", "method": "GET", "path": "/zones/{zone_id}/dns_records"},
    {"id": "create_dns_record", "name": "Create DNS Record", "method": "POST", "path": "/zones/{zone_id}/dns_records"},
    {"id": "update_dns_record", "name": "Update DNS Record", "method": "PUT", "path": "/zones/{zone_id}/dns_records/{id}"},
    {"id": "delete_dns_record", "name": "Delete DNS Record", "method": "DELETE", "path": "/zones/{zone_id}/dns_records/{id}"},
    {"id": "list_workers", "name": "List Workers", "method": "GET", "path": "/accounts/{account_id}/workers/scripts"},
    {"id": "upload_worker", "name": "Upload Worker", "method": "PUT", "path": "/accounts/{account_id}/workers/scripts/{script_name}"},
    {"id": "delete_worker", "name": "Delete Worker", "method": "DELETE", "path": "/accounts/{account_id}/workers/scripts/{script_name}"},
    {"id": "list_kv_namespaces", "name": "List KV Namespaces", "method": "GET", "path": "/accounts/{account_id}/storage/kv/namespaces"},
    {"id": "create_kv_namespace", "name": "Create KV Namespace", "method": "POST", "path": "/accounts/{account_id}/storage/kv/namespaces"},
    {"id": "delete_kv_namespace", "name": "Delete KV Namespace", "method": "DELETE", "path": "/accounts/{account_id}/storage/kv/namespaces/{id}"},
    {"id": "read_kv_value", "name": "Read KV Value", "method": "GET", "path": "/accounts/{account_id}/storage/kv/namespaces/{namespace_id}/values/{key}"},
    {"id": "write_kv_value", "name": "Write KV Value", "method": "PUT", "path": "/accounts/{account_id}/storage/kv/namespaces/{namespace_id}/values/{key}"},
    {"id": "delete_kv_value", "name": "Delete KV Value", "method": "DELETE", "path": "/accounts/{account_id}/storage/kv/namespaces/{namespace_id}/values/{key}"},
    {"id": "list_firewall_rules", "name": "List Firewall Rules", "method": "GET", "path": "/zones/{zone_id}/firewall/rules"},
    {"id": "create_firewall_rule", "name": "Create Firewall Rule", "method": "POST", "path": "/zones/{zone_id}/firewall/rules"},
    {"id": "update_firewall_rule", "name": "Update Firewall Rule", "method": "PUT", "path": "/zones/{zone_id}/firewall/rules/{id}"},
    {"id": "delete_firewall_rule", "name": "Delete Firewall Rule", "method": "DELETE", "path": "/zones/{zone_id}/firewall/rules/{id}"},
    {"id": "list_rate_limits", "name": "List Rate Limits", "method": "GET", "path": "/zones/{zone_id}/rate_limits"},
    {"id": "create_rate_limit", "name": "Create Rate Limit", "method": "POST", "path": "/zones/{zone_id}/rate_limits"},
    {"id": "get_analytics", "name": "Get Analytics", "method": "GET", "path": "/zones/{zone_id}/analytics/dashboard"},
    {"id": "list_load_balancers", "name": "List Load Balancers", "method": "GET", "path": "/zones/{zone_id}/load_balancers"},
    {"id": "create_load_balancer", "name": "Create Load Balancer", "method": "POST", "path": "/zones/{zone_id}/load_balancers"},
    {"id": "list_page_rules", "name": "List Page Rules", "method": "GET", "path": "/zones/{zone_id}/pagerules"},
    {"id": "create_page_rule", "name": "Create Page Rule", "method": "POST", "path": "/zones/{zone_id}/pagerules"},
    {"id": "list_waf_rules", "name": "List WAF Rules", "method": "GET", "path": "/zones/{zone_id}/firewall/waf/packages"},
    {"id": "get_ssl_settings", "name": "Get SSL Settings", "method": "GET", "path": "/zones/{zone_id}/settings/ssl"},
    {"id": "update_ssl_settings", "name": "Update SSL Settings", "method": "PATCH", "path": "/zones/{zone_id}/settings/ssl"},
    {"id": "list_certificates", "name": "List Certificates", "method": "GET", "path": "/zones/{zone_id}/ssl/certificate_packs"},
    {"id": "get_zone_settings", "name": "Get Zone Settings", "method": "GET", "path": "/zones/{zone_id}/settings"},
    {"id": "update_zone_settings", "name": "Update Zone Settings", "method": "PATCH", "path": "/zones/{zone_id}/settings"}
  ]
}
