    "hu - Hungarian",
    "ro - Romanian",
    "el - Greek",
    "he - Hebrew",
    "th - Thai",
    "id - Indonesian",
    "ms - Malay"
  ],
  "annotations": [
    "DMS - Degrees, Minutes, Seconds",
    "MGRS - Military Grid Reference System",
    "Maidenhead - Maidenhead Locator System",
    "Geohash",
    "What3Words",
    "Timezone",
    "Currency",
    "Sun times",
    "Calling code",
    "Flag emoji",
    "UN M.49 codes",
    "Wikidata ID"
  ]
}
