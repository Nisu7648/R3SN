{
  "name": "OpenCage Geocoding Premium",
  "version": "1.0.0",
  "description": "FREE worldwide geocoding API with forward and reverse geocoding, timezone, currency, and location data.",
  "category": "geolocation",
  "isPremium": true,
  "isFree": true,
  "authentication": {
    "type": "api_key",
    "required": true,
    "signupUrl": "https://opencagedata.com/users/sign_up"
  },
  "features": [
    "Forward geocoding (address to coordinates)",
    "Reverse geocoding (coordinates to address)",
    "Worldwide coverage",
    "Multiple languages support",
    "Country restriction",
    "Bounding box filtering",
    "Timezone information",
    "Currency data",
    "Sun times (sunrise/sunset)",
    "What3Words integration",
    "Geohash generation",
    "Calling codes",
    "Flag emojis",
    "Confidence scores",
    "Batch geocoding",
    "No rate limiting on free tier"
  ],
  "endpoints": {
    "forwardGeocode": "/geocode/v1/json",
    "reverseGeocode": "/geocode/v1/json",
    "geocodeWithLanguage": "/geocode/v1/json",
    "geocodeInCountry": "/geocode/v1/json",
    "geocodeInBounds": "/geocode/v1/json",
    "getPlaceDetails": "/geocode/v1/json",
    "batchGeocode": "/geocode/v1/json",
    "getTimezone": "/geocode/v1/json",
    "getCurrency": "/geocode/v1/json",
    "getSunTimes": "/geocode/v1/json",
    "getWhat3Words": "/geocode/v1/json",
    "getGeohash": "/geocode/v1/json",
    "getCallingCode": "/geocode/v1/json",
    "getFlag": "/geocode/v1/json",
    "searchNearby": "/geocode/v1/json",
    "getConfidence": "/geocode/v1/json"
  },
  "rateLimit": {
    "requests": 2500,
    "period": "day",
    "note": "Free tier includes 2,500 requests per day"
  },
  "supportedLanguages": [
    "en - English",
    "es - Spanish",
    "fr - French",
    "de - German",
    "it - Italian",
    "pt - Portuguese",
    "ru - Russian",
    "zh - Chinese",
    "ja - Japanese",
    "ko - Korean",
    "ar - Arabic",
    "hi - Hindi",
    "bn - Bengali",
    "pa - Punjabi",
    "te - Telugu",
    "mr - Marathi",
    "ta - Tamil",
    "tr - Turkish",
    "vi - Vietnamese",
    "pl - Polish",
    "uk - Ukrainian",
    "nl - Dutch",
    "sv - Swedish",
    "no - Norwegian",
    "da - Danish",
    "fi - Finnish",
