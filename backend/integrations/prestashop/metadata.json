      "path": "/orders/{id}",
      "description": "Get a specific order"
    },
    {
      "id": "create_order",
      "name": "Create Order",
      "method": "POST",
      "path": "/orders",
      "description": "Create a new order"
    },
    {
      "id": "update_order",
      "name": "Update Order",
      "method": "PUT",
      "path": "/orders/{id}",
      "description": "Update an order"
    },
    {
      "id": "delete_order",
      "name": "Delete Order",
      "method": "DELETE",
      "path": "/orders/{id}",
      "description": "Delete an order"
    },
    {
      "id": "list_order_states",
      "name": "List Order States",
