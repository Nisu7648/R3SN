{
  "name": "prestashop",
  "displayName": "PrestaShop",
  "version": "1.0.0",
  "description": "Complete PrestaShop integration with 45 endpoints for products, orders, customers, categories, carriers, and store management",
  "category": "ecommerce",
  "icon": "https://cdn.worldvectorlogo.com/logos/prestashop.svg",
  "authentication": {
    "type": "apikey",
    "tokenType": "Basic",
    "scopes": []
  },
  "baseUrl": "https://{store_url}/api",
  "rateLimit": {
    "quota": 500,
    "perMinute": 500,
    "throttle": "per_store"
  },
  "endpoints": [
    {
      "id": "list_products",
      "name": "List Products",
      "method": "GET",
      "path": "/products",
      "description": "Get all products"
    },
    {
      "id": "get_product",
      "name": "Get Product",
      "method": "GET",
      "path": "/products/{id}",
      "description": "Get a specific product"
    },
    {
      "id": "create_product",
      "name": "Create Product",
      "method": "POST",
      "path": "/products",
      "description": "Create a new product"
    },
    {
      "id": "update_product",
      "name": "Update Product",
      "method": "PUT",
      "path": "/products/{id}",
      "description": "Update a product"
    },
    {
      "id": "delete_product",
      "name": "Delete Product",
      "method": "DELETE",
      "path": "/products/{id}",
      "description": "Delete a product"
    },
    {
      "id": "search_products",
      "name": "Search Products",
      "method": "GET",
      "path": "/products?filter[name]=[{search_term}]",
      "description": "Search products by name"
    },
    {
      "id": "list_orders",
      "name": "List Orders",
      "method": "GET",
      "path": "/orders",
      "description": "Get all orders"
    },
    {
      "id": "get_order",
      "name": "Get Order",
