{
  "name": "kaggle_kernels",
  "displayName": "Kaggle Kernels",
  "version": "1.0.0",
  "description": "Premium Kaggle platform with 28 endpoints - FREE GPU compute with 30 hours/week GPU quota",
  "category": "cloud_compute",
  "tier": "premium",
  "icon": "https://www.kaggle.com/favicon.ico",
  "authentication": {"type": "api_key", "headers": ["X-Kaggle-Key"]},
  "baseUrl": "https://www.kaggle.com/api/v1",
  "rateLimit": {"perMinute": 200},
  "premiumFeatures": [
    "FREE GPU (P100, T4) - 30 hours/week",
    "FREE TPU access",
    "9 hours continuous runtime",
    "16GB RAM (GPU), 30GB RAM (TPU)",
    "Datasets marketplace",
    "Competition hosting",
    "Model hosting",
    "Community notebooks"
  ],
  "computeTypes": ["CPU", "GPU (P100)", "GPU (T4)", "TPU v3-8"],
  "endpoints": [
    {"id": "list_kernels", "name": "List Kernels", "method": "GET", "path": "/kernels/list"},
    {"id": "get_kernel", "name": "Get Kernel", "method": "GET", "path": "/kernels/{username}/{kernel_slug}"},
    {"id": "create_kernel", "name": "Create Kernel", "method": "POST", "path": "/kernels/push"},
    {"id": "update_kernel", "name": "Update Kernel", "method": "POST", "path": "/kernels/push"},
    {"id": "delete_kernel", "name": "Delete Kernel", "method": "DELETE", "path": "/kernels/{username}/{kernel_slug}"},
    {"id": "get_kernel_status", "name": "Get Kernel Status", "method": "GET", "path": "/kernels/status/{username}/{kernel_slug}"},
    {"id": "get_kernel_output", "name": "Get Kernel Output", "method": "GET", "path": "/kernels/output/{username}/{kernel_slug}"},
    {"id": "list_datasets", "name": "List Datasets", "method": "GET", "path": "/datasets/list"},
    {"id": "get_dataset", "name": "Get Dataset", "method": "GET", "path": "/datasets/view/{owner}/{dataset_slug}"},
    {"id": "create_dataset", "name": "Create Dataset", "method": "POST", "path": "/datasets/create/new"},
    {"id": "update_dataset", "name": "Update Dataset", "method": "POST", "path": "/datasets/create/version/{owner}/{dataset_slug}"},
    {"id": "download_dataset", "name": "Download Dataset", "method": "GET", "path": "/datasets/download/{owner}/{dataset_slug}"},
    {"id": "list_competitions", "name": "List Competitions", "method": "GET", "path": "/competitions/list"},
    {"id": "get_competition", "name": "Get Competition", "method": "GET", "path": "/competitions/{id}"},
    {"id": "submit_competition", "name": "Submit to Competition", "method": "POST", "path": "/competitions/submissions/submit/{id}"},
    {"id": "list_submissions", "name": "List Submissions", "method": "GET", "path": "/competitions/submissions/list/{id}"},
    {"id": "get_leaderboard", "name": "Get Leaderboard", "method": "GET", "path": "/competitions/leaderboard/view/{id}"},
    {"id": "list_models", "name": "List Models", "method": "GET", "path": "/models/list"},
    {"id": "get_model", "name": "Get Model", "method": "GET", "path": "/models/get/{owner}/{model_slug}"},
    {"id": "create_model", "name": "Create Model", "method": "POST", "path": "/models/create/new"},
    {"id": "update_model", "name": "Update Model", "method": "POST", "path": "/models/create/version/{owner}/{model_slug}"},
    {"id": "get_user_profile", "name": "Get User Profile", "method": "GET", "path": "/users/{username}"},
    {"id": "get_user_kernels", "name": "Get User Kernels", "method": "GET", "path": "/kernels/list?user={username}"},
    {"id": "get_user_datasets", "name": "Get User Datasets", "method": "GET", "path": "/datasets/list?user={username}"},
    {"id": "get_metadata", "name": "Get Metadata", "method": "GET", "path": "/metadata/get/{owner}/{slug}"},
    {"id": "get_quota", "name": "Get GPU Quota", "method": "GET", "path": "/quota"},
    {"id": "get_usage", "name": "Get Usage Stats", "method": "GET", "path": "/usage"},
    {"id": "search", "name": "Search", "method": "GET", "path": "/search"}
  ]
}
