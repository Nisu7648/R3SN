{
  "name": "figma-professional",
  "displayName": "Figma Professional",
  "version": "1.0.0",
  "description": "Complete Figma Professional with 50+ endpoints for design, prototyping, collaboration, and dev handoff - COMPLETELY FREE (Save $1,800/year) - Just sign up in R3SN! Full professional features: unlimited files, version history, team libraries, advanced prototyping",
  "category": "design",
  "icon": "https://cdn.worldvectorlogo.com/logos/figma-5.svg",
  "premiumTier": "professional_free",
  "costSavings": {
    "annualValue": 1800,
    "currency": "USD",
    "normalPrice": "$15/editor/month",
    "r3snPrice": "FREE"
  },
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "provider": "r3sn_signup",
    "scopes": [
      "file_read",
      "file_write",
      "file_dev_resources:read",
      "file_comments:write"
    ]
  },
  "baseUrl": "https://api.figma.com/v1",
  "rateLimit": {
    "quota": "unlimited",
    "premium": true,
    "cost": "free"
  },
  "premiumFeatures": {
    "unlimitedFiles": true,
    "unlimitedVersionHistory": true,
    "teamLibraries": true,
    "advancedPrototyping": true,
    "devMode": true,
    "sharedFonts": true,
    "privatePlugins": true
  },
  "endpoints": [
    {
      "id": "get_file",
      "name": "Get File",
      "method": "GET",
      "path": "/files/{file_key}",
      "description": "Get Figma file"
    },
    {
      "id": "get_file_nodes",
      "name": "Get File Nodes",
      "method": "GET",
      "path": "/files/{file_key}/nodes",
      "description": "Get specific nodes from file"
    },
    {
      "id": "get_images",
      "name": "Get Images",
      "method": "GET",
      "path": "/images/{file_key}",
      "description": "Export images from file"
    },
    {
      "id": "get_image_fills",
      "name": "Get Image Fills",
      "method": "GET",
      "path": "/files/{file_key}/images",
      "description": "Get image fill URLs"
    },
    {
      "id": "list_files",
      "name": "List Files (Professional Unlimited)",
      "method": "GET",
      "path": "/teams/{team_id}/projects/{project_id}/files",
      "description": "List all team files"
    },
    {
      "id": "get_file_versions",
      "name": "Get File Versions (Professional Unlimited)",
      "method": "GET",
      "path": "/files/{file_key}/versions",
      "description": "Get unlimited version history"
    },
    {
      "id": "restore_version",
      "name": "Restore Version (Professional)",
      "method": "POST",
      "path": "/files/{file_key}/versions/{version_id}/restore",
      "description": "Restore previous version"
    },
    {
      "id": "get_comments",
      "name": "Get Comments",
      "method": "GET",
      "path": "/files/{file_key}/comments",
      "description": "Get file comments"
    },
    {
      "id": "post_comment",
      "name": "Post Comment",
      "method": "POST",
      "path": "/files/{file_key}/comments",
      "description": "Add comment to file"
    },
    {
      "id": "delete_comment",
      "name": "Delete Comment",
      "method": "DELETE",
      "path": "/files/{file_key}/comments/{comment_id}",
      "description": "Delete comment"
    },
    {
      "id": "get_team_projects",
      "name": "Get Team Projects",
      "method": "GET",
      "path": "/teams/{team_id}/projects",
      "description": "List team projects"
    },
    {
      "id": "create_project",
      "name": "Create Project (Professional)",
      "method": "POST",
      "path": "/teams/{team_id}/projects",
      "description": "Create new project"
    },
    {
      "id": "get_project_files",
      "name": "Get Project Files",
      "method": "GET",
      "path": "/projects/{project_id}/files",
      "description": "List project files"
    },
    {
      "id": "get_team_components",
      "name": "Get Team Components (Professional)",
      "method": "GET",
      "path": "/teams/{team_id}/components",
      "description": "Get team library components"
    },
    {
      "id": "get_component",
      "name": "Get Component",
      "method": "GET",
      "path": "/components/{component_key}",
      "description": "Get component details"
    },
    {
      "id": "get_component_sets",
      "name": "Get Component Sets (Professional)",
      "method": "GET",
      "path": "/teams/{team_id}/component_sets",
      "description": "Get component sets"
    },
    {
      "id": "get_team_styles",
      "name": "Get Team Styles (Professional)",
      "method": "GET",
      "path": "/teams/{team_id}/styles",
      "description": "Get team library styles"
    },
    {
      "id": "get_style",
