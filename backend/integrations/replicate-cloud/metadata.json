{
  "name": "replicate_cloud",
  "displayName": "Replicate Cloud",
  "version": "1.0.0",
  "description": "Premium Replicate platform with 25 endpoints - FREE GPU access to run ML models in the cloud",
  "category": "gpu_cloud",
  "tier": "premium",
  "icon": "https://replicate.com/favicon.ico",
  "authentication": {"type": "token", "headers": ["Authorization"]},
  "baseUrl": "https://api.replicate.com/v1",
  "rateLimit": {"perMinute": 500},
  "premiumFeatures": ["FREE model inference", "1000+ pre-trained models", "Custom model deployment", "Auto-scaling", "Low latency", "Webhook support", "Version control", "API access"],
  "endpoints": [
    {"id": "list_models", "name": "List Models", "method": "GET", "path": "/models"},
    {"id": "get_model", "name": "Get Model", "method": "GET", "path": "/models/{owner}/{name}"},
    {"id": "list_model_versions", "name": "List Model Versions", "method": "GET", "path": "/models/{owner}/{name}/versions"},
    {"id": "get_model_version", "name": "Get Model Version", "method": "GET", "path": "/models/{owner}/{name}/versions/{version_id}"},
    {"id": "create_prediction", "name": "Create Prediction", "method": "POST", "path": "/predictions"},
    {"id": "get_prediction", "name": "Get Prediction", "method": "GET", "path": "/predictions/{prediction_id}"},
    {"id": "cancel_prediction", "name": "Cancel Prediction", "method": "POST", "path": "/predictions/{prediction_id}/cancel"},
    {"id": "list_predictions", "name": "List Predictions", "method": "GET", "path": "/predictions"},
    {"id": "create_deployment", "name": "Create Deployment", "method": "POST", "path": "/deployments"},
    {"id": "list_deployments", "name": "List Deployments", "method": "GET", "path": "/deployments"},
    {"id": "get_deployment", "name": "Get Deployment", "method": "GET", "path": "/deployments/{owner}/{name}"},
    {"id": "update_deployment", "name": "Update Deployment", "method": "PATCH", "path": "/deployments/{owner}/{name}"},
    {"id": "delete_deployment", "name": "Delete Deployment", "method": "DELETE", "path": "/deployments/{owner}/{name}"},
    {"id": "create_training", "name": "Create Training", "method": "POST", "path": "/trainings"},
    {"id": "get_training", "name": "Get Training", "method": "GET", "path": "/trainings/{training_id}"},
    {"id": "cancel_training", "name": "Cancel Training", "method": "POST", "path": "/trainings/{training_id}/cancel"},
    {"id": "list_trainings", "name": "List Trainings", "method": "GET", "path": "/trainings"},
    {"id": "list_collections", "name": "List Collections", "method": "GET", "path": "/collections"},
    {"id": "get_collection", "name": "Get Collection", "method": "GET", "path": "/collections/{collection_slug}"},
    {"id": "create_webhook", "name": "Create Webhook", "method": "POST", "path": "/webhooks"},
    {"id": "list_webhooks", "name": "List Webhooks", "method": "GET", "path": "/webhooks"},
    {"id": "get_webhook", "name": "Get Webhook", "method": "GET", "path": "/webhooks/{webhook_id}"},
    {"id": "delete_webhook", "name": "Delete Webhook", "method": "DELETE", "path": "/webhooks/{webhook_id}"},
    {"id": "get_account", "name": "Get Account", "method": "GET", "path": "/account"},
    {"id": "get_hardware", "name": "Get Hardware", "method": "GET", "path": "/hardware"}
  ]
}
