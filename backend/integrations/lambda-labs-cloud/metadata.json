{
  "name": "lambda_labs_cloud",
  "displayName": "Lambda Labs Cloud",
  "version": "1.0.0",
  "description": "Premium Lambda Labs GPU cloud with 30 endpoints - FREE tier with A100/H100 access for AI/ML workloads",
  "category": "gpu_cloud",
  "tier": "premium",
  "icon": "https://lambdalabs.com/favicon.ico",
  "authentication": {"type": "bearer", "tokenType": "Bearer"},
  "baseUrl": "https://cloud.lambdalabs.com/api/v1",
  "rateLimit": {"perMinute": 500},
  "premiumFeatures": ["FREE A100 access", "FREE H100 access", "1-Click Jupyter", "Pre-configured ML frameworks", "Persistent storage", "SSH access", "Team collaboration", "API access"],
  "gpuTypes": ["A100 80GB", "A100 40GB", "H100", "RTX 6000 Ada", "A10", "V100"],
  "endpoints": [
    {"id": "list_instances", "name": "List Instances", "method": "GET", "path": "/instances"},
    {"id": "get_instance", "name": "Get Instance", "method": "GET", "path": "/instances/{id}"},
    {"id": "launch_instance", "name": "Launch Instance", "method": "POST", "path": "/instance-operations/launch"},
    {"id": "terminate_instance", "name": "Terminate Instance", "method": "POST", "path": "/instance-operations/terminate"},
    {"id": "restart_instance", "name": "Restart Instance", "method": "POST", "path": "/instance-operations/restart"},
    {"id": "list_instance_types", "name": "List Instance Types", "method": "GET", "path": "/instance-types"},
    {"id": "get_instance_availability", "name": "Get Instance Availability", "method": "GET", "path": "/instance-types/{id}/availability"},
    {"id": "list_ssh_keys", "name": "List SSH Keys", "method": "GET", "path": "/ssh-keys"},
    {"id": "add_ssh_key", "name": "Add SSH Key", "method": "POST", "path": "/ssh-keys"},
    {"id": "delete_ssh_key", "name": "Delete SSH Key", "method": "DELETE", "path": "/ssh-keys/{id}"},
    {"id": "list_filesystems", "name": "List Filesystems", "method": "GET", "path": "/file-systems"},
    {"id": "create_filesystem", "name": "Create Filesystem", "method": "POST", "path": "/file-systems"},
    {"id": "delete_filesystem", "name": "Delete Filesystem", "method": "DELETE", "path": "/file-systems/{id}"},
    {"id": "get_pricing", "name": "Get Pricing", "method": "GET", "path": "/pricing"},
    {"id": "get_usage", "name": "Get Usage", "method": "GET", "path": "/usage"},
    {"id": "get_billing", "name": "Get Billing", "method": "GET", "path": "/billing"},
    {"id": "list_teams", "name": "List Teams", "method": "GET", "path": "/teams"},
    {"id": "create_team", "name": "Create Team", "method": "POST", "path": "/teams"},
    {"id": "add_team_member", "name": "Add Team Member", "method": "POST", "path": "/teams/{id}/members"},
    {"id": "remove_team_member", "name": "Remove Team Member", "method": "DELETE", "path": "/teams/{id}/members/{user_id}"},
    {"id": "list_snapshots", "name": "List Snapshots", "method": "GET", "path": "/snapshots"},
    {"id": "create_snapshot", "name": "Create Snapshot", "method": "POST", "path": "/snapshots"},
    {"id": "delete_snapshot", "name": "Delete Snapshot", "method": "DELETE", "path": "/snapshots/{id}"},
    {"id": "restore_snapshot", "name": "Restore Snapshot", "method": "POST", "path": "/snapshots/{id}/restore"},
    {"id": "get_metrics", "name": "Get Metrics", "method": "GET", "path": "/instances/{id}/metrics"},
    {"id": "get_logs", "name": "Get Logs", "method": "GET", "path": "/instances/{id}/logs"},
    {"id": "list_regions", "name": "List Regions", "method": "GET", "path": "/regions"},
    {"id": "get_quota", "name": "Get Quota", "method": "GET", "path": "/quota"},
    {"id": "request_quota_increase", "name": "Request Quota Increase", "method": "POST", "path": "/quota/increase"},
    {"id": "get_api_status", "name": "Get API Status", "method": "GET", "path": "/status"}
  ]
}
