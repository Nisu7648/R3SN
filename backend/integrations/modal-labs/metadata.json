{
  "name": "modal_labs",
  "displayName": "Modal Labs",
  "version": "1.0.0",
  "description": "Premium Modal Labs platform with 30 endpoints - FREE serverless GPU compute for ML workloads",
  "category": "gpu_cloud",
  "tier": "premium",
  "icon": "https://modal.com/favicon.ico",
  "authentication": {"type": "token", "headers": ["Authorization"]},
  "baseUrl": "https://api.modal.com/v1",
  "rateLimit": {"perMinute": 600},
  "premiumFeatures": ["FREE GPU compute", "Serverless functions", "Container support", "Auto-scaling", "Persistent volumes", "Scheduled jobs", "Webhooks", "Python-first"],
  "endpoints": [
    {"id": "list_apps", "name": "List Apps", "method": "GET", "path": "/apps"},
    {"id": "get_app", "name": "Get App", "method": "GET", "path": "/apps/{app_id}"},
    {"id": "create_app", "name": "Create App", "method": "POST", "path": "/apps"},
    {"id": "update_app", "name": "Update App", "method": "PATCH", "path": "/apps/{app_id}"},
    {"id": "delete_app", "name": "Delete App", "method": "DELETE", "path": "/apps/{app_id}"},
    {"id": "deploy_app", "name": "Deploy App", "method": "POST", "path": "/apps/{app_id}/deploy"},
    {"id": "list_functions", "name": "List Functions", "method": "GET", "path": "/functions"},
    {"id": "get_function", "name": "Get Function", "method": "GET", "path": "/functions/{function_id}"},
    {"id": "invoke_function", "name": "Invoke Function", "method": "POST", "path": "/functions/{function_id}/invoke"},
    {"id": "list_function_calls", "name": "List Function Calls", "method": "GET", "path": "/functions/{function_id}/calls"},
    {"id": "get_function_call", "name": "Get Function Call", "method": "GET", "path": "/calls/{call_id}"},
    {"id": "list_volumes", "name": "List Volumes", "method": "GET", "path": "/volumes"},
    {"id": "create_volume", "name": "Create Volume", "method": "POST", "path": "/volumes"},
    {"id": "get_volume", "name": "Get Volume", "method": "GET", "path": "/volumes/{volume_id}"},
    {"id": "delete_volume", "name": "Delete Volume", "method": "DELETE", "path": "/volumes/{volume_id}"},
    {"id": "list_secrets", "name": "List Secrets", "method": "GET", "path": "/secrets"},
    {"id": "create_secret", "name": "Create Secret", "method": "POST", "path": "/secrets"},
    {"id": "get_secret", "name": "Get Secret", "method": "GET", "path": "/secrets/{secret_id}"},
    {"id": "delete_secret", "name": "Delete Secret", "method": "DELETE", "path": "/secrets/{secret_id}"},
    {"id": "list_schedules", "name": "List Schedules", "method": "GET", "path": "/schedules"},
    {"id": "create_schedule", "name": "Create Schedule", "method": "POST", "path": "/schedules"},
    {"id": "get_schedule", "name": "Get Schedule", "method": "GET", "path": "/schedules/{schedule_id}"},
    {"id": "delete_schedule", "name": "Delete Schedule", "method": "DELETE", "path": "/schedules/{schedule_id}"},
    {"id": "list_webhooks", "name": "List Webhooks", "method": "GET", "path": "/webhooks"},
    {"id": "create_webhook", "name": "Create Webhook", "method": "POST", "path": "/webhooks"},
    {"id": "delete_webhook", "name": "Delete Webhook", "method": "DELETE", "path": "/webhooks/{webhook_id}"},
    {"id": "get_logs", "name": "Get Logs", "method": "GET", "path": "/logs"},
    {"id": "get_usage", "name": "Get Usage", "method": "GET", "path": "/usage"},
    {"id": "get_billing", "name": "Get Billing", "method": "GET", "path": "/billing"},
    {"id": "get_credits", "name": "Get Credits", "method": "GET", "path": "/credits"}
  ]
}
