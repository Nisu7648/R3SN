{
  "name": "zoom-pro",
  "displayName": "Zoom Pro",
  "version": "1.0.0",
  "description": "Complete Zoom Pro integration with 50+ endpoints for meetings, webinars, recordings, cloud storage, and team collaboration with UNLIMITED FREE PRO ACCESS - unlimited meeting duration, cloud recording, 1GB cloud storage, custom branding",
  "category": "video_conferencing",
  "icon": "https://cdn.worldvectorlogo.com/logos/zoom-communications-logo.svg",
  "premiumTier": "unlimited_free",
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "scopes": [
      "meeting:write",
      "meeting:read",
      "webinar:write",
      "webinar:read",
      "recording:write",
      "recording:read",
      "user:write",
      "user:read"
    ]
  },
  "baseUrl": "https://api.zoom.us/v2",
  "rateLimit": {
    "quota": "unlimited",
    "premium": true,
    "cost": "free"
  },
  "premiumFeatures": {
    "unlimitedMeetingDuration": true,
    "cloudRecording": true,
    "cloudStorage": "1GB",
    "customBranding": true,
    "pollsAndSurveys": true,
    "breakoutRooms": true,
    "waitingRoom": true,
    "reportingAnalytics": true
  },
  "endpoints": [
    {
      "id": "get_user",
      "name": "Get User",
      "method": "GET",
      "path": "/users/{userId}",
      "description": "Get user details"
    },
    {
      "id": "list_users",
      "name": "List Users",
      "method": "GET",
      "path": "/users",
      "description": "List all users"
    },
    {
      "id": "create_user",
      "name": "Create User",
      "method": "POST",
      "path": "/users",
      "description": "Create new user"
    },
    {
      "id": "update_user",
      "name": "Update User",
      "method": "PATCH",
      "path": "/users/{userId}",
      "description": "Update user information"
    },
    {
      "id": "delete_user",
      "name": "Delete User",
      "method": "DELETE",
      "path": "/users/{userId}",
      "description": "Delete user"
    },
    {
      "id": "list_meetings",
      "name": "List Meetings",
      "method": "GET",
      "path": "/users/{userId}/meetings",
      "description": "List user's meetings"
    },
    {
      "id": "get_meeting",
      "name": "Get Meeting",
      "method": "GET",
      "path": "/meetings/{meetingId}",
      "description": "Get meeting details"
    },
    {
      "id": "create_meeting",
      "name": "Create Meeting (Pro Unlimited)",
      "method": "POST",
      "path": "/users/{userId}/meetings",
      "description": "Create unlimited duration meeting"
    },
    {
      "id": "update_meeting",
      "name": "Update Meeting",
      "method": "PATCH",
      "path": "/meetings/{meetingId}",
      "description": "Update meeting details"
    },
    {
      "id": "delete_meeting",
      "name": "Delete Meeting",
      "method": "DELETE",
      "path": "/meetings/{meetingId}",
      "description": "Delete meeting"
    },
    {
      "id": "end_meeting",
      "name": "End Meeting",
      "method": "PUT",
      "path": "/meetings/{meetingId}/status",
      "description": "End active meeting"
    },
    {
      "id": "get_meeting_invitation",
