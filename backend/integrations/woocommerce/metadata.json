      "method": "PUT",
      "path": "/customers/{id}",
      "description": "Update a customer"
    },
    {
      "id": "delete_customer",
      "name": "Delete Customer",
      "method": "DELETE",
      "path": "/customers/{id}",
      "description": "Delete a customer"
    },
    {
      "id": "batch_customers",
      "name": "Batch Update Customers",
      "method": "POST",
      "path": "/customers/batch",
      "description": "Create, update and delete multiple customers"
    },
    {
      "id": "list_coupons",
      "name": "List Coupons",
      "method": "GET",
      "path": "/coupons",
      "description": "Get all coupons"
    },
    {
