{
  "name": "woocommerce",
  "displayName": "WooCommerce",
  "version": "1.0.0",
  "description": "Complete WooCommerce integration with 48 endpoints for products, orders, customers, coupons, shipping, and store management",
  "category": "ecommerce",
  "icon": "https://cdn.worldvectorlogo.com/logos/woocommerce.svg",
  "authentication": {
    "type": "basic",
    "tokenType": "Basic",
    "scopes": [
      "read",
      "write",
      "read_write"
    ]
  },
  "baseUrl": "https://{store_url}/wp-json/wc/v3",
  "rateLimit": {
    "quota": 100,
    "perMinute": 100,
    "throttle": "per_store"
  },
  "endpoints": [
    {
      "id": "list_products",
      "name": "List Products",
      "method": "GET",
      "path": "/products",
      "description": "Get all products"
    },
    {
      "id": "get_product",
      "name": "Get Product",
      "method": "GET",
      "path": "/products/{id}",
      "description": "Get a specific product"
    },
    {
      "id": "create_product",
      "name": "Create Product",
      "method": "POST",
      "path": "/products",
      "description": "Create a new product"
    },
    {
      "id": "update_product",
      "name": "Update Product",
      "method": "PUT",
      "path": "/products/{id}",
      "description": "Update a product"
    },
    {
      "id": "delete_product",
      "name": "Delete Product",
      "method": "DELETE",
      "path": "/products/{id}",
      "description": "Delete a product"
    },
    {
      "id": "batch_products",
      "name": "Batch Update Products",
      "method": "POST",
      "path": "/products/batch",
      "description": "Create, update and delete multiple products"
    },
    {
      "id": "list_product_variations",
      "name": "List Product Variations",
      "method": "GET",
      "path": "/products/{product_id}/variations",
      "description": "Get all variations of a product"
    },
    {
      "id": "create_product_variation",
      "name": "Create Product Variation",
      "method": "POST",
      "path": "/products/{product_id}/variations",
      "description": "Create a product variation"
    },
    {
      "id": "list_orders",
      "name": "List Orders",
      "method": "GET",
      "path": "/orders",
      "description": "Get all orders"
    },
    {
      "id": "get_order",
      "name": "Get Order",
      "method": "GET",
      "path": "/orders/{id}",
      "description": "Get a specific order"
    },
    {
      "id": "create_order",
      "name": "Create Order",
      "method": "POST",
      "path": "/orders",
      "description": "Create a new order"
    },
    {
      "id": "update_order",
      "name": "Update Order",
      "method": "PUT",
      "path": "/orders/{id}",
      "description": "Update an order"
    },
    {
      "id": "delete_order",
      "name": "Delete Order",
      "method": "DELETE",
      "path": "/orders/{id}",
      "description": "Delete an order"
    },
    {
      "id": "batch_orders",
      "name": "Batch Update Orders",
      "method": "POST",
      "path": "/orders/batch",
      "description": "Create, update and delete multiple orders"
    },
    {
      "id": "list_order_notes",
      "name": "List Order Notes",
      "method": "GET",
      "path": "/orders/{order_id}/notes",
      "description": "Get all notes for an order"
    },
    {
      "id": "create_order_note",
      "name": "Create Order Note",
      "method": "POST",
      "path": "/orders/{order_id}/notes",
      "description": "Create an order note"
    },
    {
      "id": "list_customers",
      "name": "List Customers",
      "method": "GET",
      "path": "/customers",
      "description": "Get all customers"
    },
    {
      "id": "get_customer",
