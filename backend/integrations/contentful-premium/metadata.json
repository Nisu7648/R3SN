{
  "name": "contentful_premium",
  "displayName": "Contentful Premium",
  "version": "1.0.0",
  "description": "Premium Contentful headless CMS with 30 advanced endpoints for content management, localization, and workflows",
  "category": "cms",
  "tier": "premium",
  "icon": "https://www.contentful.com/favicon.ico",
  "authentication": {"type": "bearer", "tokenType": "Bearer"},
  "baseUrl": "https://api.contentful.com",
  "rateLimit": {"perSecond": 10},
  "premiumFeatures": ["Advanced workflows", "Content scheduling", "Localization", "Webhooks", "Content preview", "Roles & permissions", "Content versioning", "GraphQL API"],
  "endpoints": [
    {"id": "get_space", "name": "Get Space", "method": "GET", "path": "/spaces/{space_id}"},
    {"id": "list_environments", "name": "List Environments", "method": "GET", "path": "/spaces/{space_id}/environments"},
    {"id": "get_environment", "name": "Get Environment", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}"},
    {"id": "list_entries", "name": "List Entries", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/entries"},
    {"id": "get_entry", "name": "Get Entry", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}"},
    {"id": "create_entry", "name": "Create Entry", "method": "POST", "path": "/spaces/{space_id}/environments/{environment_id}/entries"},
    {"id": "update_entry", "name": "Update Entry", "method": "PUT", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}"},
    {"id": "delete_entry", "name": "Delete Entry", "method": "DELETE", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}"},
    {"id": "publish_entry", "name": "Publish Entry", "method": "PUT", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}/published"},
    {"id": "unpublish_entry", "name": "Unpublish Entry", "method": "DELETE", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}/published"},
    {"id": "list_assets", "name": "List Assets", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/assets"},
    {"id": "get_asset", "name": "Get Asset", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/assets/{asset_id}"},
    {"id": "create_asset", "name": "Create Asset", "method": "POST", "path": "/spaces/{space_id}/environments/{environment_id}/assets"},
    {"id": "process_asset", "name": "Process Asset", "method": "PUT", "path": "/spaces/{space_id}/environments/{environment_id}/assets/{asset_id}/files/{locale}/process"},
    {"id": "publish_asset", "name": "Publish Asset", "method": "PUT", "path": "/spaces/{space_id}/environments/{environment_id}/assets/{asset_id}/published"},
    {"id": "list_content_types", "name": "List Content Types", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/content_types"},
    {"id": "get_content_type", "name": "Get Content Type", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/content_types/{content_type_id}"},
    {"id": "create_content_type", "name": "Create Content Type", "method": "POST", "path": "/spaces/{space_id}/environments/{environment_id}/content_types"},
    {"id": "update_content_type", "name": "Update Content Type", "method": "PUT", "path": "/spaces/{space_id}/environments/{environment_id}/content_types/{content_type_id}"},
    {"id": "list_locales", "name": "List Locales", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/locales"},
    {"id": "create_locale", "name": "Create Locale", "method": "POST", "path": "/spaces/{space_id}/environments/{environment_id}/locales"},
    {"id": "list_webhooks", "name": "List Webhooks", "method": "GET", "path": "/spaces/{space_id}/webhook_definitions"},
    {"id": "create_webhook", "name": "Create Webhook", "method": "POST", "path": "/spaces/{space_id}/webhook_definitions"},
    {"id": "delete_webhook", "name": "Delete Webhook", "method": "DELETE", "path": "/spaces/{space_id}/webhook_definitions/{webhook_id}"},
    {"id": "list_roles", "name": "List Roles", "method": "GET", "path": "/spaces/{space_id}/roles"},
    {"id": "create_role", "name": "Create Role", "method": "POST", "path": "/spaces/{space_id}/roles"},
    {"id": "list_scheduled_actions", "name": "List Scheduled Actions", "method": "GET", "path": "/spaces/{space_id}/scheduled_actions"},
    {"id": "create_scheduled_action", "name": "Create Scheduled Action", "method": "POST", "path": "/spaces/{space_id}/scheduled_actions"},
    {"id": "graphql_query", "name": "GraphQL Query", "method": "POST", "path": "/spaces/{space_id}/environments/{environment_id}/graphql"},
    {"id": "get_content_preview", "name": "Get Content Preview", "method": "GET", "path": "/spaces/{space_id}/environments/{environment_id}/entries/{entry_id}"}
  ]
}
