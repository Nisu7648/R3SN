{
  "name": "oracle_cloud_free",
  "displayName": "Oracle Cloud Free Tier",
  "version": "1.0.0",
  "description": "Premium Oracle Cloud with 40 endpoints - FREE FOREVER! 4 ARM CPUs + 24GB RAM + 200GB storage - NO CREDIT CARD REQUIRED!",
  "category": "cloud_compute",
  "tier": "premium",
  "icon": "https://www.oracle.com/favicon.ico",
  "authentication": {"type": "api_key", "headers": ["Authorization"]},
  "baseUrl": "https://cloud.oracle.com/api/v1",
  "rateLimit": {"perMinute": 500},
  "premiumFeatures": [
    "✅ FREE FOREVER - No credit card needed!",
    "✅ 4 ARM-based Ampere A1 cores (3.0 GHz)",
    "✅ 24 GB RAM",
    "✅ 200 GB block storage",
    "✅ 10 TB outbound data transfer/month",
    "✅ 2 AMD-based VMs (1/8 OCPU + 1GB RAM each)",
    "✅ Load balancer (10 Mbps)",
    "✅ Autonomous Database (2 instances)",
    "✅ Object storage (10GB)",
    "✅ Archive storage (10GB)",
    "✅ NO EXPIRATION - Use forever!"
  ],
  "freeResources": {
    "compute": "4 ARM cores + 24GB RAM (always free)",
    "storage": "200GB block + 10GB object (always free)",
    "database": "2 Autonomous DB instances (always free)",
    "networking": "10TB transfer/month (always free)",
    "loadBalancer": "1 load balancer (always free)"
  },
  "endpoints": [
    {"id": "list_instances", "name": "List Compute Instances", "method": "GET", "path": "/instances"},
    {"id": "get_instance", "name": "Get Instance", "method": "GET", "path": "/instances/{instance_id}"},
    {"id": "create_instance", "name": "Create Instance", "method": "POST", "path": "/instances"},
    {"id": "start_instance", "name": "Start Instance", "method": "POST", "path": "/instances/{instance_id}/actions/start"},
    {"id": "stop_instance", "name": "Stop Instance", "method": "POST", "path": "/instances/{instance_id}/actions/stop"},
    {"id": "reboot_instance", "name": "Reboot Instance", "method": "POST", "path": "/instances/{instance_id}/actions/reboot"},
    {"id": "terminate_instance", "name": "Terminate Instance", "method": "DELETE", "path": "/instances/{instance_id}"},
    {"id": "list_shapes", "name": "List Shapes (VM Types)", "method": "GET", "path": "/shapes"},
    {"id": "list_images", "name": "List Images", "method": "GET", "path": "/images"},
    {"id": "list_volumes", "name": "List Block Volumes", "method": "GET", "path": "/volumes"},
    {"id": "create_volume", "name": "Create Volume", "method": "POST", "path": "/volumes"},
    {"id": "attach_volume", "name": "Attach Volume", "method": "POST", "path": "/volumeAttachments"},
    {"id": "detach_volume", "name": "Detach Volume", "method": "DELETE", "path": "/volumeAttachments/{attachment_id}"},
    {"id": "delete_volume", "name": "Delete Volume", "method": "DELETE", "path": "/volumes/{volume_id}"},
    {"id": "list_vcns", "name": "List Virtual Cloud Networks", "method": "GET", "path": "/vcns"},
    {"id": "create_vcn", "name": "Create VCN", "method": "POST", "path": "/vcns"},
    {"id": "list_subnets", "name": "List Subnets", "method": "GET", "path": "/subnets"},
    {"id": "create_subnet", "name": "Create Subnet", "method": "POST", "path": "/subnets"},
    {"id": "list_security_lists", "name": "List Security Lists", "method": "GET", "path": "/securityLists"},
    {"id": "create_security_list", "name": "Create Security List", "method": "POST", "path": "/securityLists"},
    {"id": "list_buckets", "name": "List Object Storage Buckets", "method": "GET", "path": "/n/{namespace}/b"},
    {"id": "create_bucket", "name": "Create Bucket", "method": "POST", "path": "/n/{namespace}/b"},
    {"id": "upload_object", "name": "Upload Object", "method": "PUT", "path": "/n/{namespace}/b/{bucket}/o/{object}"},
    {"id": "download_object", "name": "Download Object", "method": "GET", "path": "/n/{namespace}/b/{bucket}/o/{object}"},
    {"id": "delete_object", "name": "Delete Object", "method": "DELETE", "path": "/n/{namespace}/b/{bucket}/o/{object}"},
    {"id": "list_databases", "name": "List Autonomous Databases", "method": "GET", "path": "/autonomousDatabases"},
    {"id": "create_database", "name": "Create Autonomous Database", "method": "POST", "path": "/autonomousDatabases"},
    {"id": "start_database", "name": "Start Database", "method": "POST", "path": "/autonomousDatabases/{db_id}/actions/start"},
    {"id": "stop_database", "name": "Stop Database", "method": "POST", "path": "/autonomousDatabases/{db_id}/actions/stop"},
    {"id": "list_load_balancers", "name": "List Load Balancers", "method": "GET", "path": "/loadBalancers"},
    {"id": "create_load_balancer", "name": "Create Load Balancer", "method": "POST", "path": "/loadBalancers"},
    {"id": "list_functions", "name": "List Functions", "method": "GET", "path": "/functions"},
    {"id": "create_function", "name": "Create Function", "method": "POST", "path": "/functions"},
    {"id": "invoke_function", "name": "Invoke Function", "method": "POST", "path": "/functions/{function_id}/actions/invoke"},
    {"id": "get_usage", "name": "Get Usage Stats", "method": "GET", "path": "/usage"},
    {"id": "get_cost", "name": "Get Cost Analysis", "method": "GET", "path": "/costAnalysis"},
    {"id": "get_limits", "name": "Get Service Limits", "method": "GET", "path": "/limits"},
    {"id": "list_availability_domains", "name": "List Availability Domains", "method": "GET", "path": "/availabilityDomains"},
    {"id": "get_tenancy", "name": "Get Tenancy Info", "method": "GET", "path": "/tenancies/{tenancy_id}"},
    {"id": "get_free_tier_status", "name": "Get Free Tier Status", "method": "GET", "path": "/freeTier/status"}
  ]
}
