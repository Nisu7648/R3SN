{
  "name": "paperspace_gradient",
  "displayName": "Paperspace Gradient",
  "version": "1.0.0",
  "description": "Premium Paperspace Gradient ML platform with 32 endpoints - FREE GPU hours for training and inference",
  "category": "gpu_cloud",
  "tier": "premium",
  "icon": "https://www.paperspace.com/favicon.ico",
  "authentication": {"type": "api_key", "headers": ["X-API-Key"]},
  "baseUrl": "https://api.paperspace.io",
  "rateLimit": {"perMinute": 300},
  "premiumFeatures": ["FREE GPU hours", "Jupyter notebooks", "Workflows", "Deployments", "Model registry", "Datasets", "Experiments tracking", "Team collaboration"],
  "gpuTypes": ["A100", "V100", "P4000", "RTX4000", "RTX5000", "A4000", "A6000"],
  "endpoints": [
    {"id": "list_machines", "name": "List Machines", "method": "GET", "path": "/machines/getMachines"},
    {"id": "get_machine", "name": "Get Machine", "method": "GET", "path": "/machines/getMachinePublic"},
    {"id": "create_machine", "name": "Create Machine", "method": "POST", "path": "/machines/createSingleMachinePublic"},
    {"id": "start_machine", "name": "Start Machine", "method": "POST", "path": "/machines/{id}/start"},
    {"id": "stop_machine", "name": "Stop Machine", "method": "POST", "path": "/machines/{id}/stop"},
    {"id": "destroy_machine", "name": "Destroy Machine", "method": "POST", "path": "/machines/{id}/destroyMachine"},
    {"id": "list_notebooks", "name": "List Notebooks", "method": "GET", "path": "/notebooks/list"},
    {"id": "create_notebook", "name": "Create Notebook", "method": "POST", "path": "/notebooks/create"},
    {"id": "start_notebook", "name": "Start Notebook", "method": "POST", "path": "/notebooks/{id}/start"},
    {"id": "stop_notebook", "name": "Stop Notebook", "method": "POST", "path": "/notebooks/{id}/stop"},
    {"id": "fork_notebook", "name": "Fork Notebook", "method": "POST", "path": "/notebooks/{id}/fork"},
    {"id": "list_experiments", "name": "List Experiments", "method": "GET", "path": "/experiments/list"},
    {"id": "create_experiment", "name": "Create Experiment", "method": "POST", "path": "/experiments/createExperiment"},
    {"id": "get_experiment", "name": "Get Experiment", "method": "GET", "path": "/experiments/{id}"},
    {"id": "get_experiment_logs", "name": "Get Experiment Logs", "method": "GET", "path": "/experiments/{id}/logs"},
    {"id": "list_jobs", "name": "List Jobs", "method": "GET", "path": "/jobs/list"},
    {"id": "create_job", "name": "Create Job", "method": "POST", "path": "/jobs/createJob"},
    {"id": "get_job", "name": "Get Job", "method": "GET", "path": "/jobs/{id}"},
    {"id": "clone_job", "name": "Clone Job", "method": "POST", "path": "/jobs/{id}/clone"},
    {"id": "list_deployments", "name": "List Deployments", "method": "GET", "path": "/deployments/list"},
    {"id": "create_deployment", "name": "Create Deployment", "method": "POST", "path": "/deployments/create"},
    {"id": "get_deployment", "name": "Get Deployment", "method": "GET", "path": "/deployments/{id}"},
    {"id": "update_deployment", "name": "Update Deployment", "method": "PUT", "path": "/deployments/{id}"},
    {"id": "delete_deployment", "name": "Delete Deployment", "method": "DELETE", "path": "/deployments/{id}"},
    {"id": "list_models", "name": "List Models", "method": "GET", "path": "/models/list"},
    {"id": "create_model", "name": "Create Model", "method": "POST", "path": "/models/create"},
    {"id": "get_model", "name": "Get Model", "method": "GET", "path": "/models/{id}"},
    {"id": "list_datasets", "name": "List Datasets", "method": "GET", "path": "/datasets/list"},
    {"id": "create_dataset", "name": "Create Dataset", "method": "POST", "path": "/datasets/create"},
    {"id": "get_usage", "name": "Get Usage", "method": "GET", "path": "/usage"},
    {"id": "list_teams", "name": "List Teams", "method": "GET", "path": "/teams/list"},
    {"id": "get_api_key", "name": "Get API Key", "method": "GET", "path": "/apiKeys/list"}
  ]
}
