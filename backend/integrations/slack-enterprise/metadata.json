{
  "name": "slack-enterprise",
  "displayName": "Slack Enterprise Grid",
  "version": "1.0.0",
  "description": "Complete Slack Enterprise Grid integration with 60+ endpoints for messaging, channels, workflows, admin tools, and analytics with UNLIMITED FREE ENTERPRISE ACCESS - unlimited message history, unlimited apps, advanced security, SAML SSO, compliance exports",
  "category": "team_communication",
  "icon": "https://cdn.worldvectorlogo.com/logos/slack-new-logo.svg",
  "premiumTier": "unlimited_free",
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "scopes": [
      "channels:read",
      "channels:write",
      "channels:history",
      "chat:write",
      "users:read",
      "users:write",
      "team:read",
      "admin",
      "admin.analytics:read",
      "admin.conversations:write",
      "admin.usergroups:write"
    ]
  },
  "baseUrl": "https://slack.com/api",
  "rateLimit": {
    "quota": "unlimited",
    "premium": true,
    "cost": "free"
  },
  "premiumFeatures": {
    "unlimitedMessageHistory": true,
    "unlimitedApps": true,
    "advancedSecurity": true,
    "samlSSO": true,
    "complianceExports": true,
    "advancedAnalytics": true,
    "enterpriseSupport": true,
    "dataResidency": true
  },
