{
  "name": "supabase_pro",
  "displayName": "Supabase Pro",
  "version": "1.0.0",
  "description": "Premium Supabase integration with 30 advanced endpoints for database, auth, storage, realtime, and edge functions",
  "category": "backend_service",
  "tier": "premium",
  "icon": "https://supabase.com/favicon.ico",
  "authentication": {"type": "api_key", "tokenType": "Bearer"},
  "baseUrl": "https://{project_ref}.supabase.co",
  "rateLimit": {"perMinute": 1000},
  "premiumFeatures": ["Advanced auth", "Realtime subscriptions", "Edge functions", "Storage buckets", "Database backups", "Point-in-time recovery", "Read replicas", "Custom domains"],
  "endpoints": [
    {"id": "query_table", "name": "Query Table", "method": "GET", "path": "/rest/v1/{table}"},
    {"id": "insert_row", "name": "Insert Row", "method": "POST", "path": "/rest/v1/{table}"},
    {"id": "update_row", "name": "Update Row", "method": "PATCH", "path": "/rest/v1/{table}"},
    {"id": "delete_row", "name": "Delete Row", "method": "DELETE", "path": "/rest/v1/{table}"},
    {"id": "upsert_row", "name": "Upsert Row", "method": "POST", "path": "/rest/v1/{table}"},
    {"id": "rpc_call", "name": "RPC Call", "method": "POST", "path": "/rest/v1/rpc/{function}"},
    {"id": "signup", "name": "Sign Up", "method": "POST", "path": "/auth/v1/signup"},
    {"id": "signin", "name": "Sign In", "method": "POST", "path": "/auth/v1/token"},
    {"id": "signout", "name": "Sign Out", "method": "POST", "path": "/auth/v1/logout"},
    {"id": "get_user", "name": "Get User", "method": "GET", "path": "/auth/v1/user"},
    {"id": "update_user", "name": "Update User", "method": "PUT", "path": "/auth/v1/user"},
    {"id": "reset_password", "name": "Reset Password", "method": "POST", "path": "/auth/v1/recover"},
    {"id": "verify_otp", "name": "Verify OTP", "method": "POST", "path": "/auth/v1/verify"},
    {"id": "list_buckets", "name": "List Storage Buckets", "method": "GET", "path": "/storage/v1/bucket"},
    {"id": "create_bucket", "name": "Create Bucket", "method": "POST", "path": "/storage/v1/bucket"},
    {"id": "get_bucket", "name": "Get Bucket", "method": "GET", "path": "/storage/v1/bucket/{id}"},
    {"id": "delete_bucket", "name": "Delete Bucket", "method": "DELETE", "path": "/storage/v1/bucket/{id}"},
    {"id": "list_files", "name": "List Files", "method": "POST", "path": "/storage/v1/object/list/{bucket}"},
    {"id": "upload_file", "name": "Upload File", "method": "POST", "path": "/storage/v1/object/{bucket}/{path}"},
    {"id": "download_file", "name": "Download File", "method": "GET", "path": "/storage/v1/object/{bucket}/{path}"},
    {"id": "delete_file", "name": "Delete File", "method": "DELETE", "path": "/storage/v1/object/{bucket}/{path}"},
    {"id": "move_file", "name": "Move File", "method": "POST", "path": "/storage/v1/object/move"},
    {"id": "copy_file", "name": "Copy File", "method": "POST", "path": "/storage/v1/object/copy"},
    {"id": "create_signed_url", "name": "Create Signed URL", "method": "POST", "path": "/storage/v1/object/sign/{bucket}/{path}"},
    {"id": "list_edge_functions", "name": "List Edge Functions", "method": "GET", "path": "/functions/v1"},
    {"id": "invoke_function", "name": "Invoke Edge Function", "method": "POST", "path": "/functions/v1/{function}"},
    {"id": "get_realtime_config", "name": "Get Realtime Config", "method": "GET", "path": "/realtime/v1/config"},
    {"id": "subscribe_channel", "name": "Subscribe to Channel", "method": "POST", "path": "/realtime/v1/channels"},
    {"id": "get_project_settings", "name": "Get Project Settings", "method": "GET", "path": "/v1/projects/{ref}"},
    {"id": "update_project_settings", "name": "Update Project Settings", "method": "PATCH", "path": "/v1/projects/{ref}"}
  ]
}
