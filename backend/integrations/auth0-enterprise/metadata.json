{
  "name": "auth0_enterprise",
  "displayName": "Auth0 Enterprise",
  "version": "1.0.0",
  "description": "Premium Auth0 integration with 30 enterprise endpoints for authentication, authorization, user management, and security",
  "category": "authentication",
  "tier": "premium",
  "icon": "https://auth0.com/favicon.ico",
  "authentication": {"type": "bearer", "tokenType": "Bearer"},
  "baseUrl": "https://{domain}.auth0.com/api/v2",
  "rateLimit": {"perSecond": 50},
  "premiumFeatures": ["Advanced MFA", "Passwordless auth", "Enterprise connections", "Custom domains", "Anomaly detection", "Breached password detection", "Bot detection", "Attack protection"],
  "endpoints": [
    {"id": "list_users", "name": "List Users", "method": "GET", "path": "/users"},
    {"id": "get_user", "name": "Get User", "method": "GET", "path": "/users/{id}"},
    {"id": "create_user", "name": "Create User", "method": "POST", "path": "/users"},
    {"id": "update_user", "name": "Update User", "method": "PATCH", "path": "/users/{id}"},
    {"id": "delete_user", "name": "Delete User", "method": "DELETE", "path": "/users/{id}"},
    {"id": "get_user_roles", "name": "Get User Roles", "method": "GET", "path": "/users/{id}/roles"},
    {"id": "assign_roles", "name": "Assign Roles to User", "method": "POST", "path": "/users/{id}/roles"},
    {"id": "remove_roles", "name": "Remove Roles from User", "method": "DELETE", "path": "/users/{id}/roles"},
    {"id": "get_user_permissions", "name": "Get User Permissions", "method": "GET", "path": "/users/{id}/permissions"},
    {"id": "assign_permissions", "name": "Assign Permissions", "method": "POST", "path": "/users/{id}/permissions"},
    {"id": "list_roles", "name": "List Roles", "method": "GET", "path": "/roles"},
    {"id": "create_role", "name": "Create Role", "method": "POST", "path": "/roles"},
    {"id": "update_role", "name": "Update Role", "method": "PATCH", "path": "/roles/{id}"},
    {"id": "delete_role", "name": "Delete Role", "method": "DELETE", "path": "/roles/{id}"},
    {"id": "list_clients", "name": "List Clients", "method": "GET", "path": "/clients"},
    {"id": "get_client", "name": "Get Client", "method": "GET", "path": "/clients/{id}"},
    {"id": "create_client", "name": "Create Client", "method": "POST", "path": "/clients"},
    {"id": "update_client", "name": "Update Client", "method": "PATCH", "path": "/clients/{id}"},
    {"id": "list_connections", "name": "List Connections", "method": "GET", "path": "/connections"},
    {"id": "create_connection", "name": "Create Connection", "method": "POST", "path": "/connections"},
    {"id": "update_connection", "name": "Update Connection", "method": "PATCH", "path": "/connections/{id}"},
    {"id": "delete_connection", "name": "Delete Connection", "method": "DELETE", "path": "/connections/{id}"},
    {"id": "list_rules", "name": "List Rules", "method": "GET", "path": "/rules"},
    {"id": "create_rule", "name": "Create Rule", "method": "POST", "path": "/rules"},
    {"id": "get_logs", "name": "Get Logs", "method": "GET", "path": "/logs"},
    {"id": "get_stats", "name": "Get Stats", "method": "GET", "path": "/stats/daily"},
    {"id": "list_organizations", "name": "List Organizations", "method": "GET", "path": "/organizations"},
    {"id": "create_organization", "name": "Create Organization", "method": "POST", "path": "/organizations"},
    {"id": "get_branding", "name": "Get Branding", "method": "GET", "path": "/branding"},
    {"id": "update_branding", "name": "Update Branding", "method": "PATCH", "path": "/branding"}
  ]
}
