{
  "name": "SerpAPI Premium",
  "version": "1.0.0",
  "description": "FREE Google Search Results API with 100 searches/month. Access Google, Bing, Yahoo, YouTube, and more search engines.",
  "category": "search",
  "isPremium": true,
  "isFree": true,
  "authentication": {
    "type": "api_key",
    "required": true,
    "signupUrl": "https://serpapi.com/users/sign_up"
  },
  "features": [
    "Google Search results",
    "Google Images search",
    "Google News search",
    "Google Shopping results",
    "Google Maps/Local results",
    "Google Jobs search",
    "Google Scholar search",
    "YouTube search",
    "Bing search",
    "DuckDuckGo search",
    "Baidu search",
    "Yandex search",
    "Google Trends",
    "Google Autocomplete",
    "Reverse image search",
    "Knowledge Graph data",
    "Related searches",
    "Answer boxes",
    "JSON format",
    "Real-time results"
  ],
  "endpoints": {
    "googleSearch": "/search?engine=google",
    "googleImages": "/search?engine=google_images",
    "googleNews": "/search?engine=google_news",
    "googleShopping": "/search?engine=google_shopping",
    "googleMaps": "/search?engine=google_maps",
    "googleJobs": "/search?engine=google_jobs",
    "googleScholar": "/search?engine=google_scholar",
    "youtubeSearch": "/search?engine=youtube",
    "bingSearch": "/search?engine=bing",
    "duckduckgoSearch": "/search?engine=duckduckgo",
    "baiduSearch": "/search?engine=baidu",
    "yandexSearch": "/search?engine=yandex",
    "googleTrends": "/search?engine=google_trends",
    "googleAutocomplete": "/search?engine=google_autocomplete",
    "googleReverseImage": "/search?engine=google_reverse_image",
    "getAccountInfo": "/account"
  },
  "rateLimit": {
    "requests": 100,
    "period": "month",
    "note": "Free tier includes 100 searches per month"
  },
  "supportedEngines": [
    "google",
    "google_images",
    "google_videos",
    "google_news",
    "google_shopping",
    "google_maps",
    "google_jobs",
    "google_scholar",
    "google_patents",
    "google_trends",
    "google_autocomplete",
    "google_reverse_image",
    "youtube",
    "bing",
    "bing_images",
    "bing_news",
    "yahoo",
    "duckduckgo",
    "baidu",
    "yandex",
    "ebay",
    "walmart",
    "home_depot",
    "apple_app_store",
    "google_play"
  ],
  "searchParameters": {
    "q": "Search query",
    "location": "Geographic location",
    "hl": "Language (e.g., en, es, fr)",
    "gl": "Country (e.g., us, uk, ca)",
    "num": "Number of results (1-100)",
    "start": "Pagination offset",
    "tbs": "Time-based search (qdr:d, qdr:w, qdr:m, qdr:y)",
    "safe": "Safe search (active, off)",
    "filter": "Duplicate filter (0, 1)",
    "tbm": "Search type (isch for images, nws for news)",
    "nfpr": "Personal results filter (0, 1)"
  },
  "responseFields": {
    "organic_results": "Main search results",
    "answer_box": "Featured snippet/answer",
    "knowledge_graph": "Knowledge panel data",
    "related_searches": "Related search suggestions",
    "pagination": "Pagination information",
    "search_information": "Search metadata",
    "inline_images": "Image results",
    "inline_videos": "Video results",
    "local_results": "Local/map results",
    "shopping_results": "Shopping/product results"
  }
}
