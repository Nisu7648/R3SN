{
  "name": "dropbox-business",
  "displayName": "Dropbox Business",
  "version": "1.0.0",
  "description": "Complete Dropbox Business integration with 55+ endpoints for file storage, sharing, team collaboration, and admin tools with UNLIMITED FREE BUSINESS ACCESS - unlimited storage, advanced sharing, team management, version history, smart sync",
  "category": "cloud_storage",
  "icon": "https://cdn.worldvectorlogo.com/logos/dropbox-1.svg",
  "premiumTier": "unlimited_free",
  "authentication": {
    "type": "oauth2",
    "tokenType": "Bearer",
    "scopes": [
      "account_info.read",
      "files.metadata.write",
      "files.metadata.read",
      "files.content.write",
      "files.content.read",
      "sharing.write",
      "sharing.read",
      "team_data.member",
      "team_data.team_space"
    ]
  },
  "baseUrl": "https://api.dropboxapi.com/2",
  "rateLimit": {
    "quota": "unlimited",
    "premium": true,
    "cost": "free"
  },
  "premiumFeatures": {
    "unlimitedStorage": true,
    "advancedSharing": true,
    "teamManagement": true,
    "versionHistory": "unlimited",
    "smartSync": true,
    "adminControls": true,
    "auditLogs": true,
    "prioritySupport": true
  },
  "endpoints": [
    {
      "id": "get_current_account",
      "name": "Get Current Account",
      "method": "POST",
      "path": "/users/get_current_account",
      "description": "Get current user account info"
    },
    {
      "id": "get_space_usage",
      "name": "Get Space Usage (Business Unlimited)",
      "method": "POST",
      "path": "/users/get_space_usage",
      "description": "Get storage usage"
    },
    {
      "id": "list_folder",
      "name": "List Folder",
      "method": "POST",
      "path": "/files/list_folder",
      "description": "List folder contents"
    },
    {
      "id": "list_folder_continue",
      "name": "List Folder Continue",
      "method": "POST",
      "path": "/files/list_folder/continue",
      "description": "Continue listing folder"
    },
    {
      "id": "get_metadata",
      "name": "Get File/Folder Metadata",
      "method": "POST",
      "path": "/files/get_metadata",
      "description": "Get file or folder metadata"
    },
    {
      "id": "download_file",
      "name": "Download File",
      "method": "POST",
      "path": "/files/download",
      "description": "Download file content"
    },
    {
      "id": "upload_file",
      "name": "Upload File (Business Unlimited)",
      "method": "POST",
      "path": "/files/upload",
